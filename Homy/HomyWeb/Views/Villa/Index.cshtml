@model IEnumerable<Homy.Domain.Entities.Villa>

<div class="w-100 card border-0 p-4">
	<div class="card-header bg-success bg-gradient m-lg-0 py-3">
		<div class="row">
			<div class="col-12 text-center">
				<h2 class="text-white py-2">Villa List</h2>
			</div>
		</div>
	</div>
	<div class="card-body border p-4">
		<table class="table table-bordered table-striped">
			<thead>
				<tr>
					<th>Name</th>
					<th>Price</th>
					<th>Sqft</th>
					<th>Occupancy</th>
				</tr>
			</thead>
			<tbody>
				@foreach (var obj in Model)
				{
					<tr>
						<th>@obj.Name</th>
						<th>@obj.Price</th>
						<th>@obj.Sqft</th>
						<th>@obj.Occupancy</th>
					</tr>
				}
			</tbody>
		</table>
	</div>
</div>

